{
  "manifest_version": 3,
  "name": "My Extension",
  "version": "1.0",
  "description": "A Chrome extension with a content script for specific sites.",
  "content_scripts": [
    {
      "matches": [
        "*://*.gemini.com/*",
        "*://*.deepseek.com/*",
        "*://*.groq.com/*"
      ],
      "js": ["content-script.js"]
    }
  ]
}